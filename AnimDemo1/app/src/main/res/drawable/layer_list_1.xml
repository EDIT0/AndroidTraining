<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/icLauncherBackground"
        android:width="100dp"
        android:height="100dp"
        android:bottom="0dp"
        android:drawable="@drawable/ic_launcher_background"
        android:end="0dp"
        android:gravity="center"
        android:start="0dp"
        android:top="0dp" />
    <item
        android:id="@+id/icLauncherBackgroundGrey"
        android:width="80dp"
        android:height="80dp"
        android:bottom="0dp"
        android:drawable="@drawable/ic_launcher_background_grey"
        android:end="0dp"
        android:gravity="center"
        android:start="30dp"
        android:top="30dp" />
    <item
        android:id="@+id/icLauncherForeground"
        android:width="50dp"
        android:height="50dp"
        android:bottom="0dp"
        android:drawable="@drawable/ic_launcher_foreground"
        android:end="0dp"
        android:gravity="center"
        android:start="110dp"
        android:top="110dp" />
</layer-list>